#include <stdio.h>

int main() {
    int n, sum = 0;
    
    printf("Enter the value of n: ");
    scanf("%d", &n);
    
    // Iterate through the odd numbers from 1 to n
    for (int i = 1; i <= n; i += 2) {
        sum += i;
    }
    
    printf("Sum of the series is: %d\n", sum);
    
    return 0;
}
