#include <stdio.h>

int main() {
    int n = 10, i;
    int fib1 = 0, fib2 = 1, fib3;
    
    printf("Fibonacci series: %d %d ", fib1, fib2); // Print the first two numbers
    
    for(i = 2; i < n; i++) { // Start the loop from the third number
        fib3 = fib1 + fib2;
        printf("%d ", fib3);
        fib1 = fib2;
        fib2 = fib3;
    }
    
    return 0;
}
