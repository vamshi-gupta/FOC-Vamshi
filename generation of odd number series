#include <stdio.h>

int main() {
   int n, i, oddNum;
   
   printf("Enter the number of odd numbers you want to generate: ");
   scanf("%d", &n);
   
   printf("The first %d odd numbers are:\n", n);
   
   oddNum = 1;
   
   for (i = 1; i <= n; i++) {
      printf("%d ", oddNum);
      oddNum += 2;
   }
   
   return 0;
}
